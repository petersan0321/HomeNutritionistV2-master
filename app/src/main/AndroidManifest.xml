<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.home.homenutritionist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignupActivity"
            android:label="SignupActivity">
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="HomeActivity">
        </activity>
        <activity
            android:name=".CaloriesCaltorActivity"
            android:label="CaloriesCaltorActivity">
        </activity>
        <activity
            android:name=".MenuActivity"
            android:label="MenuActivity">
        </activity>
        <activity
            android:name=".NutritionistBlogActivity"
            android:label="NutritionistBlogActivity">
        </activity>
        <activity
            android:name=".NutritionQuizActivity"
            android:label="NutritionQuizActivity">
        </activity>
    </application>

</manifest>